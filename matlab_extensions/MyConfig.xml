<?xml version="1.0" encoding="utf-8"?>
<CustomKeySet derivedfrom="Mac" modifieddefault="false">
   <Context id="Global">
      <Action id="caret-previous-word">
         <Stroke alt="on" code="VK_H" ctrl="on"/>
         <Stroke code="VK_LEFT" ctrl="on"/>
         <Stroke code="VK_KP_LEFT" ctrl="on"/>
      </Action>
      <Action id="selection-backward">
         <Stroke code="VK_LEFT" shift="on"/>
         <Stroke code="VK_KP_LEFT" shift="on"/>
         <Stroke code="VK_H" ctrl="on" shift="on"/>
      </Action>
      <Action id="kill-line">
         <Stroke code="VK_BACK_SPACE" meta="on" shift="on" sysctrl="on"/>
      </Action>
      <Action id="move-track-down">
         <Stroke code="VK_J" ctrl="on"/>
      </Action>
      <Action id="delete-next">
         <Stroke code="VK_DELETE"/>
      </Action>
      <Action id="previous-history-item">
         <Stroke code="VK_UP"/>
         <Stroke code="VK_KP_UP"/>
      </Action>
      <Action id="select-previous-tab">
         <Stroke code="VK_P" ctrl="on"/>
      </Action>
      <Action id="collapse-all-folds"/>
      <Action id="select-all">
         <Stroke code="VK_A" ctrl="on" meta="on" sysctrl="on"/>
      </Action>
      <Action id="remove-word-previous">
         <Stroke code="VK_W" ctrl="on"/>
      </Action>
      <Action id="caret-down">
         <Stroke code="VK_J" ctrl="on"/>
      </Action>
      <Action id="select-next-tab">
         <Stroke code="VK_N" ctrl="on"/>
      </Action>
      <Action id="selection-up">
         <Stroke code="VK_UP" shift="on"/>
         <Stroke code="VK_KP_UP" shift="on"/>
         <Stroke code="VK_K" ctrl="on" shift="on"/>
      </Action>
      <Action id="zoom-minus">
         <Stroke code="VK_SUBTRACT"/>
         <Stroke code="VK_MINUS" meta="on" sysctrl="on"/>
      </Action>
      <Action id="collapse-all"/>
      <Action id="caret-next-word">
         <Stroke alt="on" code="VK_L" ctrl="on"/>
         <Stroke code="VK_RIGHT" ctrl="on"/>
         <Stroke code="VK_KP_RIGHT" ctrl="on"/>
      </Action>
      <Action id="goto-position"/>
      <Action id="page-up">
         <Stroke code="VK_U" ctrl="on"/>
      </Action>
      <Action id="caret-forward">
         <Stroke code="VK_RIGHT"/>
         <Stroke code="VK_KP_RIGHT"/>
      </Action>
      <Action id="page-down">
         <Stroke code="VK_D" ctrl="on"/>
         <Stroke code="VK_V" ctrl="on"/>
      </Action>
      <Action id="zoom-plus">
         <Stroke code="VK_ADD"/>
         <Stroke code="VK_EQUALS" meta="on" sysctrl="on"/>
      </Action>
      <Action id="clear-command-window">
         <Stroke code="VK_L" ctrl="on"/>
      </Action>
      <Action id="goto-line">
         <Stroke code="VK_G" meta="on" sysctrl="on"/>
      </Action>
      <Action id="caret-backward">
         <Stroke code="VK_H" ctrl="on"/>
         <Stroke code="VK_LEFT"/>
         <Stroke code="VK_KP_LEFT"/>
      </Action>
      <Action id="next-cell">
         <Stroke alt="on" code="VK_J" ctrl="on"/>
      </Action>
      <Action id="generic-delete"/>
      <Action id="find-next"/>
      <Action id="prev-cell">
         <Stroke alt="on" code="VK_K" ctrl="on"/>
      </Action>
      <Action id="caret-up">
         <Stroke code="VK_K" ctrl="on"/>
      </Action>
      <Action id="selection-forward">
         <Stroke code="VK_RIGHT" shift="on"/>
         <Stroke code="VK_KP_RIGHT" shift="on"/>
         <Stroke code="VK_L" ctrl="on" shift="on"/>
      </Action>
      <Action id="clear-text-at-prompt">
         <Stroke code="VK_ESCAPE"/>
         <Stroke code="VK_OPEN_BRACKET" ctrl="on"/>
      </Action>
      <Action id="selection-down">
         <Stroke code="VK_DOWN" shift="on"/>
         <Stroke code="VK_KP_DOWN" shift="on"/>
         <Stroke code="VK_J" ctrl="on" shift="on"/>
      </Action>
      <Action id="next-history-item">
         <Stroke code="VK_DOWN"/>
         <Stroke code="VK_KP_DOWN"/>
      </Action>
   </Context>
   <Context id="MATLABEditor">
      <Action id="caret-up">
         <Stroke code="VK_UP"/>
         <Stroke code="VK_KP_UP"/>
      </Action>
      <Action id="caret-down">
         <Stroke code="VK_DOWN"/>
         <Stroke code="VK_KP_DOWN"/>
      </Action>
      <Action id="caret-forward">
         <Stroke code="VK_L" ctrl="on"/>
      </Action>
   </Context>
   <Context id="CommandWindow">
      <Action follows="None" id="caret-up">
         <Stroke code="VK_UP"/>
         <Stroke code="VK_KP_UP"/>
      </Action>
      <Action follows="None" id="caret-down">
         <Stroke code="VK_DOWN"/>
         <Stroke code="VK_KP_DOWN"/>
      </Action>
   </Context>
</CustomKeySet>